<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Origin</th>
      <th>Publisher</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    <% @characters.each do |character| %>
      <tr>
        <td><%= character.name %></td>
        <td><%= character.origin.origin %></td>
        <td><%= character.publisher.name %></td>
        <td><%= image_tag character.image, class: "img-thumbnail" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
