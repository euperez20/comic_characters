<%= render '/layouts/layout' %>
<p style="color: green"><%= notice %></p>


<h1>Publishers</h1>

<%= paginate @publishers, theme: 'twitter-bootstrap-4' %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Details</th>
      
    </tr>
  </thead>
  <tbody>
    <% @publishers.each do |publisher| %>
      <tr>
        <td><%= publisher.id %></td>
        <td><%= publisher.name %></td>
       
        <%# <td> %>
          <%# <%= link_to "Show", characters_path(publisher_id: publisher.id), class: "btn btn-primary" %>
        <%# </td> %>
      </tr>
    <% end %>
  </tbody>
</table>
